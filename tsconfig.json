{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/indexjtsx",
    "app/(auth)/login/index.tsx",
    "app/(tabs)/index.tsx",
    "app/(auth)/signup/signup.tsx",
    "app/(auth)/login/login.tsx",
    "app/(auth)/forgotpassword/forgotpassword.tsx"
, "app/(main)/home.jsx"  ]
}
